<Window x:Class="Walterlv.Demo.Devices.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Walterlv.Demo.Devices"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock>
            <Run Text="BiosSerialRun: " />
            <Run x:Name="BiosSerialRun" />
            <LineBreak />
            <Run Text="BiosVersionRun: " />
            <Run x:Name="BiosVersionRun" />
            <LineBreak />
            <Run Text="BiosDateRun: " />
            <Run x:Name="BiosDateRun" />
        </TextBlock>
    </Grid>
</Window>
